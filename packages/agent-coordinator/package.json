{
  "name": "@verifiai/agent-coordinator",
  "version": "0.1.0",
  "description": "Multi-agent coordination with LangGraph for VerifiAI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
