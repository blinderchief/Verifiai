{
  "name": "@verifiai/proof-engine",
  "version": "0.1.0",
  "description": "ZK proof generation engine for VerifiAI Protocol",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.26.0",
    "@verifiai/core": "workspace:*",
    "onnxruntime-node": "^1.16.3",
    "snarkjs": "^0.7.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/snarkjs": "^0.7.9",
    "eslint": "^8.55.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
